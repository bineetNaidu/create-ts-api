{
  "name": "create-ts-api",
  "version": "1.7.1",
  "description": "A utility command to setup nodejs TS api",
  "main": "index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bineetNaidu/create-ts-api.git"
  },
